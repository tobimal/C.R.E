<table class="table">
		<tr><%= attachment_image_tag(end_user, :profile_image, :fill, 100, 100, fallback: "NO_IMAGE.png") %></tr>

		<tr>
			<th>name</th>
			<th><%= end_user.name %></th>
		</tr>

		<tr>
			<th>introduction</th>
			<th><%= end_user.introduction %></th>
		</tr>
</table>


<% if end_user == current_end_user %>
  <div class='row'>
    <%= link_to 'プロフィール編集',edit_publics_end_user_path(end_user),class: "btn btn-primary" %>
  </div>
<% end %>
<div class='row'>
    <%= link_to 'お気に入り一覧',publics_favorite_path(end_user),class: "btn btn-primary" %>
  </div>